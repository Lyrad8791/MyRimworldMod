<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--============================ New Trees ============================-->

  <ThingDef ParentName="VG_TreewoodBase">
    <defName>PlantTreeIronwood</defName>
    <label>ironwood tree</label>
    <description>Ironwood trees got their name from the legendary strength of the wood. This is the most commonly used material for making spears and tools.</description>
    <graphicData>
      <texPath>Things/Tree/TreeIronwood</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.15, 0.3, 0.1)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <Beauty>3</Beauty>
    </statBases>
    <ingestible>
      <nutrition>1</nutrition>
    </ingestible>
    <plant>
      <leaflessGraphicPath>Things/Tree/Tree_Leafless/TreeIronwood_Leafless</leaflessGraphicPath>
      <growDays>30</growDays>
      <harvestedThingDef>VG_IronWoodLog</harvestedThingDef>
      <harvestWork>700</harvestWork>
      <harvestYield>55</harvestYield>
	<wildBiomes>
		<TemperateForest>0.01</TemperateForest>
    		<BorealForest>0.01</BorealForest>
	</wildBiomes>
       <sowResearchPrerequisites>
        <li>flowersandtrees</li>
      </sowResearchPrerequisites>
   </plant>
  </ThingDef>



</Defs>