<?xml version="1.0" encoding="UTF-8"?>
<Defs>

<!-- SOYLENT GREEN -->
	<RecipeDef>
		<defName>makesoylentgreen</defName>
		<label>Make Soylent Green</label>
		<description>Make Soylent Green from "High Energy Plankton"®.</description>
		<jobString>Making Soylent Green.</jobString>
		<workAmount>500</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>ButcherFlesh</effectWorking>
		<soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Human_Meat</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<products>
			<soylentgreen>5</soylentgreen>
		</products>
		<skillRequirements>
			<Cooking>4</Cooking>
		</skillRequirements>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Human_Meat</li>
			</thingDefs>
			<specialFiltersToDisallow>
				<li>AllowRotten</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<workSkill>Cooking</workSkill>
		<researchPrerequisite>soylentmaking</researchPrerequisite>
	</RecipeDef>
<!-- SOYLENT GREEN -->

