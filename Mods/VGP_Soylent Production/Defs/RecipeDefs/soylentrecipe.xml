	<RecipeDef>
		<defName>MakeMealSurvivalSoylent</defName>
		<label>Make Packaged Survival Meal</label>
		<description>Makes a packaged survival meal from a combination of soylent types and/or pemmican. Much of the ingredients are wasted.</description>
		<jobString>Making survival meal.</jobString>
		<workAmount>500</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>soylentgreen</li>
						<li>soylentblue</li>
						<li>soylentpurple</li>
						<li>soylentred</li>
						<li>soylentyellow</li>
						<li>Pemmican</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
		</ingredients>
		<products>
			<MealSurvivalPack>1</MealSurvivalPack>
		</products>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
		<fixedIngredientFilter>
			<thingDefs>
				<li>soylentgreen</li>
				<li>soylentblue</li>
				<li>soylentpurple</li>
				<li>soylentred</li>
				<li>soylentyellow</li>
				<li>Pemmican</li>
			</thingDefs>
			<specialFiltersToDisallow>
				<li>AllowRotten</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<workSkill>Cooking</workSkill>
		<researchPrerequisite>soylentmakingadvanced</researchPrerequisite>
	</RecipeDef>
</Defs>