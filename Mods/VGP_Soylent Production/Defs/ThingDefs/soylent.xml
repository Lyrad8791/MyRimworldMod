<?xml version="1.0" encoding="UTF-8"?>
<Defs>

<!-- SOYLENT GREEN -->
	<ThingDef ParentName="ResourceBase">
		<defName>soylentgreen</defName>
		<label>Soylent Green</label>
		<description>Soylent Green - made from "High Energy Plankton"Â®.</description>
		<stackLimit>750</stackLimit>
		<thingClass>ThingWithComps</thingClass>
		<category>Item</category>
		<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Mass>0.007</Mass>
			<DeteriorationRate>2</DeteriorationRate>
			<MarketValue>1</MarketValue>
		</statBases>
		<altitudeLayer>Item</altitudeLayer>
		<tickerType>Rare</tickerType>
		<socialPropernessMatters>true</socialPropernessMatters>
		<graphicData>
			<texPath>Things/Item/SoylentGreen</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<alwaysHaulable>true</alwaysHaulable>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>120</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
<!--			<li Class="CompProperties_Ingredients" />	-->
			<li Class="CompProperties_Forbiddable" />
			<li Class="CompProperties_FoodPoisonable" />
		</comps>
		<pathCost>15</pathCost>
		<resourceReadoutPriority>Last</resourceReadoutPriority>
		<drawGUIOverlay>true</drawGUIOverlay>
		<ingestible>
			<foodType>Meal, Processed</foodType>
			<preferability>MealAwful</preferability>
			<nutrition>0.05</nutrition>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>
		<thingCategories>
			<li>MeatRaw</li>
		</thingCategories>
	</ThingDef>
<!-- SOYLENT GREEN -->

