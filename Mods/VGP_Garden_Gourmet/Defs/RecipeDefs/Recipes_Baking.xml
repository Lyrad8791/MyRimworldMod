         <li>
            <filter>
               <thingDefs>
                  <li>Flour</li>
               </thingDefs>
            </filter>
            <count>12</count>
         </li>
		<li>
            <filter>
               <thingDefs>
                  <li>Sugar</li>
               </thingDefs>
            </filter>
            <count>5</count>
         </li>
      </ingredients>
      <products>
         <sweetbun>4</sweetbun>
      </products>
