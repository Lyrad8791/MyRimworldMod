	<jobString>Baking muffins.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<effectWorking>Cook</effectWorking>
     <workAmount>1500</workAmount>
     <soundWorking>Recipe_CookMeal</soundWorking>          
<allowMixingIngredients>false</allowMixingIngredients>
	<recipeUsers>
         <li>Oven</li>
         <li>ElectricOven</li>
     </recipeUsers>

	<ingredients>
	  <li>
	    <filter>
	      <thingDefs>
	        <li>Flour</li>
	      </thingDefs>
	    </filter>
	    <count>8</count>
	  </li>
	<li>
	    <filter>
	      <thingDefs>
	        	<li>Sugar</li>
	      </thingDefs>
	    </filter>
	    <count>4</count>
	  </li>
	<li>
		<filter>
			<categories>
				<li>FruitFoodRaw</li>
			</categories>
