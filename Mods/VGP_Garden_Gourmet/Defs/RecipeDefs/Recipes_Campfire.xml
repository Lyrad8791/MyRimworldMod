<?xml version="1.0" encoding="UTF-8"?>

<Defs>


  <RecipeDef>
    <defName>GrillKabob</defName>
    <label>grill two kabobs</label>
    <description>Meats and/or vegetables skewered on a stick and grilled to perfection.</description>
    <jobString>Grilling kabob.</jobString>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>0.6</count>
      </li>
    </ingredients>
    <products>
      <Kabob>2</Kabob>
    </products>
    <fixedIngredientFilter>
      <categories>
        <li>PlantFoodRaw</li>
        <li>MeatRaw</li>
      </categories>
      <disallowedSpecialFilters>
        <li>AllowPlantFood</li>
        <li>AllowRotten</li>
      </disallowedSpecialFilters>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>PlantFoodRaw</li>
        <li>MeatRaw</li>
      </categories>
      <exceptedThingDefs>
        <li>Human_Meat</li>
           <li>Megaspider_Meat</li>
     </exceptedThingDefs>
    </defaultIngredientFilter>
    <workSkill>Cooking</workSkill>
	<recipeUsers>
         <li>ElectricStove</li>
	    <li>FueledStove</li>
	    <li>Campfire</li>
  	    <li>VG_Campfire</li>
    </recipeUsers>
  </RecipeDef>


