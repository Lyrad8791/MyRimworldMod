<RecipeDef>
	<defName>PickleVegetable</defName>
	<label>pickle ten vegetables</label>
	<description>Preserve ten vegetables in a salty brine to prevent spoiling.</description>
	<jobString>Pickling food.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<workAmount>1000</workAmount>
		<effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>          <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawPotatoes</li>
            <li>Rawbean</li>
            <li>RawCorn</li>
            <li>Rawsnowbeet</li>
          </thingDefs>
        </filter>
        <count>40</count>
      </li>
	<li>
	  <filter>
		<thingDefs>
			<li>ChunkSalt</li>
		</thingDefs>
	  </filter>
		<count>5</count>
	</li>
	</ingredients>
	<fixedIngredientFilter>
	  <thingDefs>
            <li>RawPotatoes</li>
            <li>RawCorn</li>
            <li>Rawbean</li>
            <li>Rawsnowbeet</li>
		 <li>ChunkSalt</li>
	  </thingDefs>
      <disallowedSpecialFilters>
        <li>AllowPlantFood</li>
        <li>AllowNonSmeltableWeapons</li>
	   <li>AllowRotten</li>
      </disallowedSpecialFilters>
	</fixedIngredientFilter>
	<products>
	  <PickledVeg>10</PickledVeg>
	</products>
    <defaultIngredientFilter>
      <thingDefs>
            <li>RawPotatoes</li>
            <li>RawCorn</li>
             <li>Rawbean</li>
           <li>Rawsnowbeet</li>
    		  <li>ChunkSalt</li>
       </thingDefs>
      <exceptedCategories>
        <li>EggsFertilized</li>
    </exceptedCategories>
      <exceptedThingDefs>
        <li>PickledVeg</li>
      </exceptedThingDefs>
    </defaultIngredientFilter>
	<workSkill>Cooking</workSkill>
	<recipeUsers>
         <li>Canningstove</li>
         <li>HandCanningstove</li>
     </recipeUsers>
</RecipeDef>


<RecipeDef>
	<defName>SaltMeat</defName>
	<label>salt ten meats</label>
	<description>Preserve meats in salt to prevent spoiling.</description>
	<jobString>Salting meat.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<workAmount>1000</workAmount>
		<effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>          <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>10</count>
      </li>
	<li>
	  <filter>
		<thingDefs>
			<li>ChunkSalt</li>
		</thingDefs>
	  </filter>
		<count>5</count>
	</li>
	</ingredients>
	<fixedIngredientFilter>
	  <categories>
		<li>MeatRaw</li>
	  </categories>
     	  <thingDefs>
		<li>ChunkSalt</li>
	  </thingDefs>
	  <disallowedSpecialFilters>
        <li>AllowPlantFood</li>
        <li>AllowNonSmeltableWeapons</li>
        <li>AllowRotten</li>
      </disallowedSpecialFilters>
	</fixedIngredientFilter>
	<products>
	  <SaltedMeat>10</SaltedMeat>
	</products>
    <defaultIngredientFilter>
      <categories>
        <li>MeatRaw</li>
      </categories>
	 <thingDefs>
		<li>ChunkSalt</li>
	 </thingDefs>
      <exceptedCategories>
        <li>EggsFertilized</li>
      </exceptedCategories>
      <exceptedThingDefs>
        <li>Human_Meat</li>
        <li>SaltedMeat</li>
      </exceptedThingDefs>
    </defaultIngredientFilter>
	<workSkill>Cooking</workSkill>
	<recipeUsers>
         <li>Canningstove</li>
         <li>HandCanningstove</li>
    </recipeUsers>
</RecipeDef>

<RecipeDef>
	<defName>VG_VeganEgg</defName>
	<label>make Vegan Egg Substitute</label>
    <description>Vegan Egg Substitute is a egg shape product made from tofu, potatoes, or banana. Used in place of eggs in baking and other recipes.</description>
	<jobString>Pickling food.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<workAmount>700</workAmount>
		<effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>          <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Rawbanana</li>
            <li>RawPotatoes</li>
            <li>Rawtofu</li>
          </thingDefs>
        </filter>
        <count>.5</count>
      </li>
	</ingredients>
	<fixedIngredientFilter>
	  <thingDefs>
            <li>Rawbanana</li>
            <li>RawPotatoes</li>
            <li>Rawtofu</li>
	  </thingDefs>
      <disallowedSpecialFilters>
        <li>AllowNonSmeltableWeapons</li>
	   <li>AllowRotten</li>
      </disallowedSpecialFilters>
	</fixedIngredientFilter>
	<products>
	  <VeganEgg>2</VeganEgg>
	</products>
    <defaultIngredientFilter>
      <thingDefs>
            <li>Rawbanana</li>
            <li>RawPotatoes</li>
            <li>Rawtofu</li>
       </thingDefs>
    </defaultIngredientFilter>
	<workSkill>Cooking</workSkill>
	<recipeUsers>
         <li>Canningstove</li>
         <li>HandCanningstove</li>
     </recipeUsers>
</RecipeDef>


</Defs>
