<?xml version="1.0" encoding="utf-8" ?>
<Defs>



<ThingDef ParentName="VG_PlantDef">
	<defName>Plantwheat</defName>
	<label>wheat plant</label>
	<description>Wheat is a cereal grain, can be milled into flour.</description>
    <graphicData>
      <texPath>Things/Plants/wheatPlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <plant>
      <harvestedThingDef>Rawwheat</harvestedThingDef>
	<harvestYield>9</harvestYield>
      <sowMinSkill>5</sowMinSkill>
      <topWindExposure>0.1</topWindExposure>
      <growDays>4</growDays>
      <fertilitySensitivity>0.6</fertilitySensitivity>
    </plant>
  </ThingDef>


