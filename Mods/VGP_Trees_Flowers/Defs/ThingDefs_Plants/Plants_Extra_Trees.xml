<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <!--============================ New Trees ============================-->


  <ThingDef ParentName="VG_TreewoodBase">
    <defName>PlantTreeJapaneseMaple</defName>
    <label>japanese maple tree</label>
    <description>These beautiful trees have branches that twist and turn unpredictably and brilliant orange and red foliage that draws the eye.</description>
    <graphicData>
      <texPath>Things/Tree/TreeJapaneseMaple</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.15, 0.3, 0.1)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <Beauty>3</Beauty>
    </statBases>
    <ingestible>
      <nutrition>1</nutrition>
    </ingestible><plant>
      <leaflessGraphicPath>Things/Tree/Tree_Leafless/TreeJapaneseMaple_Leafless</leaflessGraphicPath>
      <growDays>18</growDays>
      <harvestWork>700</harvestWork>
      <harvestYield>20</harvestYield>
       <sowResearchPrerequisites>
        <li>flowersandtrees</li>
      </sowResearchPrerequisites>
       <sowTags>
        <li>Ground</li>
      </sowTags>
  </plant>
  </ThingDef>


