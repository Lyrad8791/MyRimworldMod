<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <!--=========================== New Flowers ==============================-->

  <ThingDef ParentName="VG_PlantBase">
    <defName>PlantAlliumGiganteum</defName>
    <label>allium giganteum</label>
    <description>A beautiful cultivated flower, dark purple bulb.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>14</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Flowers/AlliumGiganteum</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
      <nutrition>0.10</nutrition>
    </ingestible>
    <plant>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <sowWork>400</sowWork>
      <sowTags>
        <li>Decorative</li>
        <li>Ground</li>
      </sowTags>
      <growMinGlow>0.3</growMinGlow>
      <topWindExposure>0.1</topWindExposure>
      <growDays>1.5</growDays>
      <lifespanFraction>7</lifespanFraction>
      <fertilitySensitivity>1.0</fertilitySensitivity>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
      <sowResearchPrerequisites>
        <li>flowersandtrees</li>
      </sowResearchPrerequisites>
    </plant>
  </ThingDef>

