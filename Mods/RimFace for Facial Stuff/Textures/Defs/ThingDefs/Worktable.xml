<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>FSStylist</defName>
    <label>Face styling bench</label>
    <thingClass>FacialStuff.FaceStyling_Bench.FaceStyler</thingClass>
    <tradeability>Sellable</tradeability>
    <description>Place where faces go wild!</description>
    <graphicData>
      <texPath>Clutter/Tables/StylingBench</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,1)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <costList>
      <WoodLog>50</WoodLog>
      <Steel>20</Steel>
	  <Component>1</Component>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <minifiedDef>MinifiedFurniture</minifiedDef>
    <statBases>
      <WorkToBuild>2000</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Cleanliness>-15</Cleanliness>
    </statBases>
    <size>(2,1)</size>
  <!--  <designationCategory>Misc</designationCategory>-->
    <passability>PassThroughOnly</passability>
    <pathCost>60</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <comps>
      <li>
        <compClass>FacialStuff.CompFaceEditor</compClass>
      </li>
    </comps>
  </ThingDef>
</Defs>