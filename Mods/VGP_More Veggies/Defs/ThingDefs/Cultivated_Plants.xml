<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	
	<!-- New crop types -->
	


<ThingDef ParentName="VG_PlantDef">
		<defName>Planteggplant</defName>
		<label>eggplant plant</label>
		<description>Purple variety of the eggplant. Bitter when eaten raw. </description>
    <graphicData>
      <texPath>Things/Plants/eggplantPlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <plant>
      <harvestedThingDef>Raweggplant</harvestedThingDef>
	<harvestYield>7</harvestYield>
      <sowMinSkill>4</sowMinSkill>
      <growDays>4.5</growDays>
      <fertilitySensitivity>0.6</fertilitySensitivity>
<immatureGraphicPath>Things/Immature/eggplantPlant</immatureGraphicPath>
    </plant>

  </ThingDef>



