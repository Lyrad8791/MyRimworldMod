<Defs>
  
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedShimmershroom</defName>
    <label>extract shimmershroom seeds</label>
    <description>Extract seeds from Shimmershroom.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawShimmershroom</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawShimmershroom</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedShimmershroom>3</SeedShimmershroom>
    </products>
    <researchPrerequisite>SK_FungiII</researchPrerequisite>
  </RecipeDef>
  
